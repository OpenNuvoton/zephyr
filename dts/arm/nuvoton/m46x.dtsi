#include <arm/armv7-m.dtsi>
#include <mem.h>
#include <zephyr/dt-bindings/gpio/gpio.h>
#include <zephyr/dt-bindings/i2c/i2c.h>
#include "m46x/sys.h"
#include "m46x/sys_reg.h"
#include "m46x/clk.h"
#include "m46x/clk_reg.h"

/ {
	chosen {
		zephyr,flash-controller = &fmc;
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-m4f";
			reg = <0>;
		};
	};

	sram0: memory@20000000 {
		compatible = "mmio-sram";
        reg = <0x20000000 DT_SIZE_K(512)>;
	};

	sysclk: system-clock {
		compatible = "fixed-clock";
		clock-frequency = <200000000>;
		#clock-cells = <0>;
	};
    
 
	soc {
		scc: system-clock-controller@40000200 {
			compatible = "nuvoton,numaker-scc";
			label = "SCC";
			reg = <0x40000200 0x100>;
			#clock-cells = <0>;
			/* hxt = "enable"; */
			/* lxt = "enable"; */
			clk-pclkdiv = <(CLK_PCLKDIV_APB0DIV_DIV2 | CLK_PCLKDIV_APB1DIV_DIV2)>;
			core-clock = <200000000>;

			pcc: peripheral-clock-controller {
				compatible = "nuvoton,numaker-pcc";
				label = "PCC";
				#clock-cells = <3>;
			};
		};

        fmc: flash-controller@4000c000 {
            compatible = "nuvoton,numaker-fmc";
            label = "FLASH_CTRL";
            reg = <0x4000c000 0x110>;
            #address-cells = <1>;
            #size-cells = <1>;

            flash0: flash@0 {
                compatible = "soc-nv-flash";
                label = "FLASH_NUMAKER";
                reg = <0 DT_SIZE_K(1024)>;
                erase-block-size = <4096>;
                write-block-size = <4>;
            };
        };

		uart0: serial@40070000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40070000 0x1000>;
			interrupts = <36 0>;
            reset = <UART0_RST>;
            clocks = <&pcc UART0_MODULE CLK_CLKSEL1_UART0SEL_HIRC CLK_CLKDIV0_UART0(1)>;
			status = "disabled";
			label = "UART_0";
		};

		uart1: serial@40071000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40071000 0x1000>;
			interrupts = <37 0>;
            reset = <UART1_RST>;
            clocks = <&pcc UART1_MODULE CLK_CLKSEL1_UART1SEL_HIRC CLK_CLKDIV0_UART1(1)>;
			status = "disabled";
			label = "UART_1";
		};

		uart2: serial@40072000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40072000 0x1000>;
			interrupts = <48 0>;
            reset = <UART2_RST>;
            clocks = <&pcc UART2_MODULE CLK_CLKSEL3_UART2SEL_HIRC CLK_CLKDIV4_UART2(1)>;
			status = "disabled";
			label = "UART_2";
		};

		uart3: serial@40073000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40073000 0x1000>;
			interrupts = <49 0>;
            reset = <UART3_RST>;
            clocks = <&pcc UART3_MODULE CLK_CLKSEL3_UART3SEL_HIRC CLK_CLKDIV4_UART3(1)>;
			status = "disabled";
			label = "UART_3";
		};

		uart4: serial@40074000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40074000 0x1000>;
			interrupts = <74 0>;
            reset = <UART4_RST>;
            clocks = <&pcc UART4_MODULE CLK_CLKSEL3_UART4SEL_HIRC CLK_CLKDIV4_UART4(1)>;
			status = "disabled";
			label = "UART_4";
		};

		uart5: serial@40075000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40075000 0x1000>;
			interrupts = <75 0>;
            reset = <UART5_RST>;
            clocks = <&pcc UART5_MODULE CLK_CLKSEL3_UART5SEL_HIRC CLK_CLKDIV4_UART5(1)>;
			status = "disabled";
			label = "UART_5";
		};

		uart6: serial@40076000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40076000 0x1000>;
			interrupts = <102 0>;
            reset = <UART6_RST>;
            clocks = <&pcc UART6_MODULE CLK_CLKSEL3_UART6SEL_HIRC CLK_CLKDIV4_UART6(1)>;
			status = "disabled";
			label = "UART_6";
		};

		uart7: serial@40077000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40077000 0x1000>;
			interrupts = <103 0>;
            reset = <UART7_RST>;
            clocks = <&pcc UART7_MODULE CLK_CLKSEL3_UART7SEL_HIRC CLK_CLKDIV4_UART7(1)>;
			status = "disabled";
			label = "UART_7";
		};
        
		uart8: serial@40078000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40078000 0x1000>;
			interrupts = <99 0>;
            reset = <UART8_RST>;
            clocks = <&pcc UART8_MODULE CLK_CLKSEL2_UART8SEL_HIRC CLK_CLKDIV5_UART8(1)>;
			status = "disabled";
			label = "UART_8";
		};
        
		uart9: serial@40079000 {
			compatible = "nuvoton,numaker-uart";
			reg = <0x40079000 0x1000>;
			interrupts = <100 0>;
            reset = <UART9_RST>;
            clocks = <&pcc UART9_MODULE CLK_CLKSEL2_UART9SEL_HIRC CLK_CLKDIV5_UART9(1)>;
			status = "disabled";
			label = "UART_9";
		};        

		i2c0: i2c@40080000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40080000 0x1000>;
			interrupts = <38 0>;
			reset = <I2C0_RST>;
			clocks = <&pcc I2C0_MODULE 0 0>;
			status = "disabled";
			label = "I2C_0";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c1: i2c@40081000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40081000 0x1000>;
			interrupts = <39 0>;
			reset = <I2C1_RST>;
			clocks = <&pcc I2C1_MODULE 0 0>;
			status = "disabled";
			label = "I2C_1";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c2: i2c@40082000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40082000 0x1000>;
			interrupts = <82 0>;
			reset = <I2C2_RST>;
			clocks = <&pcc I2C2_MODULE 0 0>;
			status = "disabled";
			label = "I2C_2";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c3: i2c@40083000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40083000 0x1000>;
			interrupts = <83 0>;
			reset = <I2C3_RST>;
			clocks = <&pcc I2C3_MODULE 0 0>;
			status = "disabled";
			label = "I2C_3";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4: i2c@40084000 {
			compatible = "nuvoton,numaker-i2c";
			clock-frequency = <I2C_BITRATE_STANDARD>;
			reg = <0x40084000 0x1000>;
			interrupts = <118 0>;
			reset = <I2C4_RST>;
			clocks = <&pcc I2C4_MODULE 0 0>;
			status = "disabled";
			label = "I2C_4";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		pinctrl: pin-controller@40000500 {
			compatible = "nuvoton,numaker-pinctrl";
			reg = <0x40000500 0xa0>;
			#address-cells = <1>;
			#size-cells = <1>;
			status = "okay";
			label = "PINCTRL";
		};
		gpioa: gpio@40004000 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004000 0x40>;
            clocks = <&pcc GPA_MODULE 0 0>;
			status = "disabled";
			interrupts = <16 2>;
			label = "GPIOA";
		};

		gpiob: gpio@40004040 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004040 0x40>;
            clocks = <&pcc GPB_MODULE 0 0>;
			status = "disabled";
			interrupts = <17 2>;
			label = "GPIOB";
		};

		gpioc: gpio@40004080 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004080 0x40>;
            clocks = <&pcc GPC_MODULE 0 0>;
			status = "disabled";
			interrupts = <18 2>;
			label = "GPIOC";
		};

		gpiod: gpio@400040c0 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x400040c0 0x40>;
            clocks = <&pcc GPD_MODULE 0 0>;
			status = "disabled";
			interrupts = <19 2>;
			label = "GPIOD";
		};

		gpioe: gpio@40004100 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004100 0x40>;
            clocks = <&pcc GPE_MODULE 0 0>;
			status = "disabled";
			interrupts = <20 2>;
			label = "GPIOE";
		};

		gpiof: gpio@40004140 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004140 0x40>;
            clocks = <&pcc GPF_MODULE 0 0>;
			status = "disabled";
			interrupts = <21 2>;
			label = "GPIOF";
		};

		gpiog: gpio@40004180 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004180 0x40>;
            clocks = <&pcc GPG_MODULE 0 0>;
			status = "disabled";
			interrupts = <72 2>;
			label = "GPIOG";
		};

		gpioh: gpio@400041c0 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x400041c0 0x40>;
            clocks = <&pcc GPH_MODULE 0 0>;
			status = "disabled";
			interrupts = <88 2>;
			label = "GPIOH";
		};

		gpioi: gpio@40004200 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004200 0x40>;
            clocks = <&pcc GPI_MODULE 0 0>;
			status = "disabled";
			interrupts = <110 2>;
			label = "GPIOI";
		};

		gpioj: gpio@40004240 {
			compatible = "nuvoton,numaker-gpio";
			gpio-controller;
			#gpio-cells = <2>;
			reg = <0x40004240 0x40>;
            clocks = <&pcc GPJ_MODULE 0 0>;
			status = "disabled";
			interrupts = <61 2>;
			label = "GPIOJ";
		};
        
	};
};

&nvic {
	arm,num-irq-priority-bits = <4>;
};
